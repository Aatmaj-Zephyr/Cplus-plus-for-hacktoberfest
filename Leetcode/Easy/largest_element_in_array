#include <bits/stdc++.h>
using namespace std;

int largest(vector<int> &arr, int n)
{
    sort(arr.begin(), arr.end()); // sorting in ascending order
    return arr[n - 1];            // last will be the largest
}

int main()
{
    int ans;
    vector<int> arr = {4, 5, 6, 88, 44, 890, 567};
    int n = arr.size();
    ans = largest(arr, n);
    cout << ans;
    return 0;
}
